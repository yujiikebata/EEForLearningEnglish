{
  "manifest_version": 2,
  "name": "EEForLearningEnglish",
  "version": "0.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["bower_components/jquery/dist/jquery.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}