{
  "manifest_version": 2,
  "name": "EEForLearningEnglish",
  "version": "0.0.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["bower_components/jquery/dist/jquery.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus"],
  "icons": {
    "16": "icon16.png"
  }
}